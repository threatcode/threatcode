. /usr/sbin/tc-common

if [ "$1" == "" ]; then
        curl -K /opt/tc/conf/elasticsearch/curl.config -s -k -L https://localhost:9200/_template/* | jq .
else
        curl -K /opt/tc/conf/elasticsearch/curl.config -s -k -L https://localhost:9200/_template/$1 | jq .
fi
